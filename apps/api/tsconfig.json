{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "rootDir": "src",
    "outDir": "dist",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "../../node_modules/@types", "./src/types"]
  },
  "include": ["src"]
}
